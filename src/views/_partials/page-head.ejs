<div class="page-head d-flex justify-content-between align-items-center flex-wrap gap-3">
    <div>
        <h2><%- title %></h2>
        <% if (typeof subtitle !== 'undefined' && subtitle) { %>
            <p><%= subtitle %></p>
        <% } %>
    </div>
    <% if (typeof actions !== 'undefined' && actions) { %>
        <div><%- actions %></div>
    <% } %>
</div>
