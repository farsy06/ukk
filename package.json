{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "ukk",
  "version": "1.0.0",
  "description": "eSarpra - Aplikasi web untuk mengelola sistem peminjaman alat laboratorium dengan Node.js, Express.js, dan MySQL",
  "main": "index.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "create-admin": "node scripts/create-admin.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:api": "jest --testPathPattern=api",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "copy:bootstrap": "mkdir -p public/css public/js && copy node_modules/bootstrap/dist/css/bootstrap.min.css public/css/ && copy node_modules/bootstrap/dist/js/bootstrap.bundle.min.js public/js/",
    "build": "npm run copy:bootstrap"
  },
  "keywords": [
    "ukk",
    "nodejs",
    "express",
    "mysql",
    "peminjaman",
    "alat-laboratorium",
    "web-app",
    "crud",
    "authentication"
  ],
  "author": "Farisya Fatanansyah",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@dr.pogodin/csurf": "^1.16.8",
    "bcrypt": "^6.0.0",
    "bootstrap": "^5.3.8",
    "connect-flash": "^0.1.1",
    "dotenv": "^17.2.4",
    "ejs": "^4.0.1",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.19.0",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "mysql2": "^3.17.0",
    "node-cache": "^5.1.2",
    "pdfkit": "^0.17.2",
    "prompt-sync": "^4.2.0",
    "sequelize": "^6.37.7",
    "validator": "^13.15.26",
    "winston": "^3.19.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2"
  }
}
