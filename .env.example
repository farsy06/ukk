# =========================
# Application
# =========================
APP_NAME="ToolShare Pro"
APP_ENV=development
APP_PORT=3000
APP_URL=http://localhost:3000

# =========================
# Database (MySQL)
# =========================
DB_HOST=localhost
DB_PORT=3306
DB_NAME=ukk
DB_USER=root
DB_PASS=

# Sequelize config
DB_DIALECT=mysql
DB_LOGGING=false

# =========================
