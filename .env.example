# =========================
# Security
# =========================
BCRYPT_SALT_ROUNDS=10
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# =========================
# Cache
# =========================
CACHE_TTL=600

# =========================
# Logging
# =========================
LOG_LEVEL=info
